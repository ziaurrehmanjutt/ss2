<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<csp>
            <mode>
                <storefront>
                    <report_only>0</report_only>
                    
                </storefront>
                <admin>
                    <report_only>0</report_only>
                </admin>
            </mode>
        </csp>
		<themecore>
			<general>
				<primary_color>
					<theme_color>FFD200</theme_color>
					<text_on_theme_color>222222</text_on_theme_color>
					<link_on_theme_color>222222</link_on_theme_color>
					<linkhover_on_theme_color>000000</linkhover_on_theme_color>
				</primary_color>
				
				<bodyfont_group>
					<body_font>google_font</body_font>
					<body_googlefont_link>&lt;link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600,700" rel="stylesheet"&gt;</body_googlefont_link>
					<body_googlefont_family>font-family: 'Open Sans', sans-serif;</body_googlefont_family>
					<body_googlefont_weight>400</body_googlefont_weight>
					<font_size>14px</font_size>
					<body_link_color>222222</body_link_color>
					<body_link_hover_color>FFCC00</body_link_hover_color>
					<body_text_color>666666</body_text_color>
				</bodyfont_group>
				
				<background_group>
					<body_background_color>FFFFFF</body_background_color>
					<use_background_image>0</use_background_image>
					<body_background_image></body_background_image>
					<background_repeat>repeat</background_repeat>
					<background_position>left top</background_position>
				</background_group>
				
				<element_googlefont_group>
					<use_element_googlefont>0</use_element_googlefont>
					<element_googlefont_link></element_googlefont_link>
					<element_googlefont_family></element_googlefont_family>
					<element_googlefont_weight></element_googlefont_weight>
					<google_font_targets></google_font_targets>
				</element_googlefont_group>
				
				<navigation_group>
					<menu_style>css</menu_style>
					<menu_ontop>0</menu_ontop>
				</navigation_group>
				
				<mobile_layout>
					<mobile_break_point>1023</mobile_break_point>
					<logo_mobile></logo_mobile>
					<logomobile_width>100</logomobile_width>
					<logomobile_height>35</logomobile_height>
				</mobile_layout>
			</general>
			
			<theme_layout>
				<max_width>1170px</max_width>
				<use_boxed_layout>0</use_boxed_layout>
				<boxed_background_color>FFFFFF</boxed_background_color>
			</theme_layout>
			
			<product_listing>
				<category_one_column>
					<one_column_1921>4</one_column_1921>
					<one_column_1681_1920>4</one_column_1681_1920>
					<one_column_1441_1680>4</one_column_1441_1680>
					<one_column_1200_1440>4</one_column_1200_1440>
					<one_column_992_1199>3</one_column_992_1199>
					<one_column_768_991>3</one_column_768_991>
					<one_column_481_767>2</one_column_481_767>
					<one_column_0_480>1</one_column_0_480>
				</category_one_column>
				
				<category_two_column>
					<two_column_1921>3</two_column_1921>
					<two_column_1681_1920>3</two_column_1681_1920>
					<two_column_1441_1680>3</two_column_1441_1680>
					<two_column_1200_1440>3</two_column_1200_1440>
					<two_column_992_1199>3</two_column_992_1199>
					<two_column_768_991>3</two_column_768_991>
					<two_column_481_767>2</two_column_481_767>
					<two_column_0_480>1</two_column_0_480>
				</category_two_column>
				
				<category_three_column>
					<three_column_1921>2</three_column_1921>
					<three_column_1681_1920>2</three_column_1681_1920>
					<three_column_1441_1680>2</three_column_1441_1680>
					<three_column_1200_1440>2</three_column_1200_1440>
					<three_column_992_1199>2</three_column_992_1199>
					<three_column_768_991>3</three_column_768_991>
					<three_column_481_767>2</three_column_481_767>
					<three_column_0_480>1</three_column_0_480>
				</category_three_column>
			</product_listing>
			
			<product_detail>
				<image_box_group>
					<thumbs_style>horizontal</thumbs_style>
					<image_loop>1</image_loop>
					<image_keyboard>1</image_keyboard>
					<image_navigation>1</image_navigation>
					<image_fullscreen>1</image_fullscreen>
					<thumbs_width>110</thumbs_width>
					<thumbs_height>110</thumbs_height>
					<thumbs_navigation>1</thumbs_navigation>
					<thumbs_margin>10</thumbs_margin>
					<thumbs_borderwidth>1</thumbs_borderwidth>
					<breakpoints_width>1200</breakpoints_width>
					<use_zoom_image>1</use_zoom_image>
					<zoom_width>400</zoom_width>
				</image_box_group>
				
				<related_upsell_group>
					<show_related>1</show_related>
					<show_upsell>1</show_upsell>
				</related_upsell_group>
				
				<tabs_group>
					<tab_style>default</tab_style>
					<show_custom_tab>0</show_custom_tab>
					<custom_tab_name>Custom Tabs</custom_tab_name>
					<customtab_content>Write your content here...</customtab_content>
				</tabs_group>
			</product_detail>
			
			<advanced>
				<backtotop_group>
					<show_backtotop>1</show_backtotop>
				</backtotop_group>
				
				<product_group>
					<show_addtocart_button>1</show_addtocart_button>
					<show_wishlist_button>1</show_wishlist_button>
					<show_compare_button>1</show_compare_button>
					<show_newlabel>1</show_newlabel>
					<show_salelabel>1</show_salelabel>
					<show_discount>0</show_discount>
				</product_group>
				
				<newsletter_group>
					<show_newsletter_popup>1</show_newsletter_popup>
				</newsletter_group>
				
				<copyright_group>
					<custom_copyright>0</custom_copyright>
					<copyright_content></copyright_content>
				</copyright_group>
				
				<developer_group>
					<enable_custom_css>0</enable_custom_css>
					<custom_css></custom_css>
					<enable_custom_js>0</enable_custom_js>
					<custom_js></custom_js>
				</developer_group>
				
				<lazyload_group>
					<enable_ladyloading>1</enable_ladyloading>
				</lazyload_group>
			</advanced>
		</themecore>
	</default>
</config>